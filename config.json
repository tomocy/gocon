{
    "ociVersion": "1.0.0",
    "process": {
      "terminal": false,
      "user": {
        "uid": 0,
        "gid": 0
      },
      "args": [
        "/bin/ps"
      ],
      "env": [
        "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
      ],
      "cwd": "/"
    },
    "root": {
      "path": "rootfs"
    },
    "linux": {},
    "mounts": [
      {
        "destination": "/proc",
        "type": "proc",
        "source": "proc"
      }
    ]
  }